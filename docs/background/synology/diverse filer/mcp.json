{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "homey": {
      "type": "http",
      "url": "http://localhost:8765/"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "docker": {
      "command": "npx",
      "args": ["-y", "@thelord/mcp-server-docker-npx"],
      "env": {
        "DOCKER_HOST": "ssh://REDACTED_USERNAME@REDACTED_SERVER_IP"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@henkey/postgres-mcp-server",
        "postgresql://appuser:apppass123@REDACTED_SERVER_IP:5433/appdb"
      ]
    },
    "fetch": {
      "command": "npx",
      "args": ["-y", "mcp-fetch-server"]
    }
  }
}
